<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="RegressionSuite_Parallel"
       parallel="tests"
       thread-count="4"
       verbose="1">

    <test name="Regression_Login">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="pagestest.Login"/>
        </classes>
    </test>

    <test name="Regression_Login_Excel">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="pagestest.LoginExceldataprovider"/>
        </classes>
    </test>

    <test name="Regression_Login_JSON">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="pagestest.LoginJson"/>
        </classes>
    </test>

</suite>



