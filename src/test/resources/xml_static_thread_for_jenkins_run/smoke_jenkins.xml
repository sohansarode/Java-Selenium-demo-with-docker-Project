<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GroupedSuite_Parallel"
       parallel="tests"
       thread-count="2">

  <test name="Smoke_Login">
    <groups>
      <run>
        <include name="smoke"/>
      </run>
    </groups>
    <classes>
      <class name="pagestest.Login"/>
    </classes>
  </test>

  <test name="SmokeTests">
    <groups>
      <run>
        <include name="smoke"/>
      </run>
    </groups>
    <classes>
      <class name="pagestest.LoginExceldataprovider"/>
    </classes>
  </test>

</suite>
